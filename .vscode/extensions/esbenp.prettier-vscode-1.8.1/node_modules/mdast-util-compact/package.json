{
  "name": "mdast-util-compact",
  "version": "1.0.2",
  "description": "Make an MDAST tree compact",
  "license": "MIT",
  "keywords": [
    "mdast",
    "utility",
    "compact",
    "node"
  ],
  "repository": "syntax-tree/mdast-util-compact",
  "bugs": "https://github.com/syntax-tree/mdast-util-compact/issues",
  "author": "Titus Wormer <tituswormer@gmail.com> (http://wooorm.com)",
  "contributors": [
    "Titus Wormer <tituswormer@gmail.com> (http://wooorm.com)"
  ],
  "files": [
    "index.js"
  ],
  "dependencies": {
    "unist-util-visit": "^1.1.0"
  },
  "devDependencies": {
    "browserify": "^16.0.0",
    "nyc": "^12.0.0",
    "prettier": "^1.14.2",
    "remark-cli": "^5.0.0",
    "remark-preset-wooorm": "^4.0.0",
    "tape": "^4.0.0",
    "tinyify": "^2.4.3",
    "unist-builder": "^1.0.2",
    "xo": "^0.22.0"
  },
  "scripts": {
    "format": "remark . -qfo && prettier --write '**/*.js' && xo --fix",
    "build-bundle": "browserify . -s mdastUtilCompact > mdast-util-compact.js",
    "build-mangle": "browserify . -s mdastUtilCompact -p tinyify > mdast-util-compact.min.js",
    "build": "npm run build-bundle && npm run build-mangle",
    "test-api": "node test",
    "test-coverage": "nyc --reporter lcov tape test.js",
    "test": "npm run format && npm run build && npm run test-coverage"
  },
  "prettier": {
    "tabWidth": 2,
    "useTabs": false,
    "singleQuote": true,
    "bracketSpacing": false,
    "semi": false,
    "trailingComma": "none"
  },
  "xo": {
    "prettier": true,
    "esnext": false,
    "ignores": [
      "mdast-util-compact.js"
    ]
  },
  "nyc": {
    "check-coverage": true,
    "lines": 100,
    "functions": 100,
    "branches": 100
  },
  "remarkConfig": {
    "plugins": [
      "preset-wooorm"
    ]
  }

